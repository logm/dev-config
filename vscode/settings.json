{
  "debug.console.fontFamily": "'Cascadia Code', MesloLGS NF",
  "debug.onTaskErrors": "showErrors",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorBlinking": "expand",
  "editor.cursorSurroundingLines": 5,
  "editor.definitionLinkOpensInPeek": true,
  "editor.detectIndentation": false,
  "editor.find.autoFindInSelection": "multiline",
  "editor.fontFamily": "'Cascadia Code', 'Fira Code', 'MesloLGS NF', 'Meslo L for Powerline', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": false,
  "editor.hover.delay": 150,
  "editor.insertSpaces": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelZoom": false,
  "editor.quickSuggestions": {
    "strings": true,
    "other": true,
    "comments": true
  },
  "editor.suggest.filterGraceful": true,
  "editor.snippetSuggestions": "inline",
  "editor.quickSuggestionsDelay": 100,
  "editor.renderLineHighlight": "all",
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 4,
  "editor.wordWrap": "on",
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.visible": 0,
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "html.format.wrapLineLength": 0,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
  "remote.downloadExtensionsLocally": true,
  "streamdeck.serverHost": "0.0.0.0",
  "streamdeck.serverPort": 48969,
  "telemetry.enableTelemetry": false,
  "terminal.external.windowsExec": "C:\\WINDOWS\\System32\\wsl.exe",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontFamily": "'Cascadia Code' , MesloLGS NF",
  "terminal.integrated.rendererType": "canvas",
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
  "window.openFilesInNewWindow": "on",
  "window.restoreFullscreen": true,
"window.titleBarStyle": "custom",
  "workbench.editor.openPositioning": "first",
  "workbench.editor.untitled.labelFormat": "name",
  "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.view.alwaysShowHeaderActions": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.semicolons": "remove",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "terminal.integrated.fontSize": 15,
  "remote.SSH.remotePlatform": {
    "10.0.0.94": "linux"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "todohighlight.isEnable": false,
  "timeline.excludeSources": [],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.tabCompletion": "on",
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter-notebook",
      "filenamePattern": "*.ipynb"
    }
  ],
  "workbench.colorCustomizations": {
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#FBF305",
    "terminal.ansiYellow": "#e6de05",
    "terminal.ansiBrightRed": "#DD0907",
    "terminal.ansiRed": "#DD0907",
    "terminalCursor.foreground": "#C0C0C0e6",
    "terminalCursor.background": "#c0c0c0e3",
    "terminal.ansiWhite": "#C0C0C0",
    "terminal.foreground": "#c0c0c0",
    "terminal.ansiBrightBlack": "#808080",
    "terminal.ansiMagenta": "#8324ff",
    "terminal.ansiBrightGreen": "#64DD17",
    "terminal.ansiGreen": "#43A047",
    "terminal.ansiBrightMagenta": "#7f40d1",
    "terminal.ansiBlue": "#006be6",
    "terminal.ansiBrightCyan": "#02ABEA",
    "terminal.ansiCyan": "#02ABEA",
    "terminal.ansiBrightBlue": "#000ad3",
    "terminal.ansiBlack": "#000000",
    "tab.activeBackground": "#62626A",
    "tab.activeBorder": "#0059ff",
    // "tab.unfocusedActiveBackground": "#62626abe",
    // "tab.inactiveBackground": "#1e1e1e",
    "tab.inactiveForeground": "#D8D8D8",
    "statusBar.background": "#225e86",
    "statusBarItem.remoteBackground": "#0059ff",
  },
  "docsView.documentationView.updateMode": "live",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": false,
  "css.lint.float": "warning",
  "editor.formatOnSave": true,
  "workbench.colorTheme": "GitHub Dark",
  "editor.acceptSuggestionOnEnter": "smart",
  "typescript.format.semicolons": "remove",
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
	  "javascript": "javascriptreact",
	}
}